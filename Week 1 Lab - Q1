package weekonelab;

/**
 *
 * @author Ahmed
 */
public class FisherYates {

    public static void shuffle(Object[] array) {
        int unshuffledArray = array.length;
        for (int i = 0; i < unshuffledArray; i++) {
            int s = i + (int) (Math.random() * (unshuffledArray - i));

            Object temp = array[s];
            array[s] = array[i];
            array[i] = temp;

        }
    }
    public static void main(String[] args) {
        
        String[] num_array = {"5","3","8","6","1","9","2","7"};
        
        FisherYates.shuffle(num_array);
        
        for (int i = 0; i < num_array.length; i++) {
            System.out.print(num_array[i] + " ");
        }
    }
}

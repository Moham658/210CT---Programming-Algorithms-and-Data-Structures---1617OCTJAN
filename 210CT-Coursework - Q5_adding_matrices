A = [[3,5,9],
     [13 ,10,3],
     [17,4,5]]

B = [[10,16,3],
     [2,13,67],
     [21,14,3]]



def addMatrices(A, B):
    """Function that adds two matrices"""
    result = []
    for i in range(len(A)): #for loop that iterates through rows of matrices(list of lists)
        result_row = [] # empty list for new row input
        for j in range(len(A[0])): # 
            result.append(A[i][j]+B[i][j])
        result.append(result_row)
    return result


print(add(A, B))

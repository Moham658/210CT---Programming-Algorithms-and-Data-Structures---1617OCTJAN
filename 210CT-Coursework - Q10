List = [3,4,6,8,9,2,4,5,1,0,6,4,5,6]


def sub_seq_generator(L):
    sub_seq = []
    sub_seq_count = 1
    for i in range(len(List)-1):
        if List[i]>List[i+1]:
            sub_seq.append(sub_seq_count)
            sub_seq_count = 1
        else:
            sub_seq_count +=1
    sub_seq.append(sub_seq_count)   
    return sub_seq


print(sub_seq_generator(List))
